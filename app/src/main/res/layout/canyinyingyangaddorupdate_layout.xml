<?xml version="1.0" encoding="utf-8"?>
<com.design.appproject.widget.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/add_box_style"
    android:fillViewport="true">
    <com.design.appproject.widget.MyLinearLayout
        style="@style/add_form_style"
        android:orientation="vertical">
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="菜品名称"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.BottomSpinner
                android:id="@+id/caipinmingcheng_bs"
                style="@style/add_item_picker_box_style"
                android:text="请选择菜品名称"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/caipinfenlei_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="菜品分类"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/caipinfenlei_et"
            style="@style/add_item_input_style"
            android:hint="菜品分类"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            android:id="@+id/tupian_ll"
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:id="@+id/tupian_tv"
                android:text="图片"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.MyImageView
                android:id="@+id/tupian_ifv"
                style="@style/add_item_img_style"
                android:src="@mipmap/upload"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/kaluli_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="卡路里(cal)"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/kaluli_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="卡路里(cal)"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/tanshui_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="碳水(100g)"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/tanshui_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="碳水(100g)"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/danbaizhi_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="蛋白质(100g)"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/danbaizhi_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="蛋白质(100g)"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/zhifang_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="脂肪(100g)"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/zhifang_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="脂肪(100g)"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yingyangsu_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="营养素(100g)"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yingyangsu_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="营养素(100g)"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="营养级别"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.BottomSpinner
                android:id="@+id/yingyangjibie_bs"
                style="@style/add_item_picker_box_style"
                android:text="请选择营养级别"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFrameLayout
        style="@style/add_btn_box_style">
        <com.design.appproject.widget.MyButton
            android:id="@+id/submit_btn"
            android:text="提交"
            android:gravity="center"
        style="@style/add_btn_submit_style"/>
    </com.design.appproject.widget.MyFrameLayout>
    </com.design.appproject.widget.MyLinearLayout>
</com.design.appproject.widget.MyScrollView>
