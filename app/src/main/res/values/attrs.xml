<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="CommonTitleBarView">
        <attr name="common_title" format="string" />
        <attr name="common_smalltitle" format="string" />
        <attr name="common_titleColor" format="color" />
        <attr name="common_leftSrc" format="reference" />
        <attr name="common_leftVisible" format="boolean" />
        <attr name="common_rightSrc" format="reference" />
        <attr name="common_rightSrcSec" format="reference" />
        <attr name="common_rightText" format="string" />
        <attr name="common_sRightTextBackground" format="reference" />
        <attr name="common_leftTextSub" format="string" />
        <attr name="common_leftText" format="string" />
        <attr name="common_rightTextColor" format="color" />
        <attr name="common_leftTextColor" format="color" />
        <attr name="common_rightVisible" format="boolean" />
        <attr name="common_lineColor" format="color" />
        <attr name="common_lineVisible" format="boolean" />
        <attr name="common_whiteBar" format="boolean" />
        <attr name="common_barBg" format="color" />
        <attr name="common_subtitle" format="string" />
        <attr name="common_isTranslucent" format="boolean" /><!--        是否沉浸式状态-->
    </declare-styleable>

    <declare-styleable name="CommonGrideImageView">
        <attr name="common_showDeleteIcon" format="boolean" />   <!--是否展示删除按钮-->
        <attr name="common_imageWidth" format="dimension" />
        <attr name="common_imageHeight" format="dimension" />
        <attr name="common_spacing" format="dimension" /> <!--图片间距-->
        <attr name="common_row_count" format="integer" /> <!--单行最多显示个数-->
        <attr name="common_max_count" format="integer" /> <!--总最多显示个数-->
    </declare-styleable>

    <declare-styleable name="CommonCustomEditView"><!---->
        <attr name="common_hasCount" format="boolean" /><!-- 是否有字数统计-->
        <attr name="common_maxLength" format="integer" min="0" /><!-- 最大字数限制-->
        <attr name="common_hint" format="string" />
    </declare-styleable>

    <declare-styleable name="CommonFormatContentView"><!---->
        <attr name="common_formatType" format="enum">
            <enum name="comment" value="0" />
            <enum name="reply" value="1" />
        </attr>
        <attr name="common_moreTextCanClick" format="boolean" /><!-- 更多按钮是否可以点击-->
    </declare-styleable>

